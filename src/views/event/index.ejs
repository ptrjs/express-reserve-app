<%- include('../components/header') %>
<%- include('../components/navbar', { navs }) %>

<h1>Event</h1>
<form action="/event/create">
  <input type="submit" value="Buat">
</form>
<ul id="list">
  <% events.forEach(x => { %>
  <li>
    <form method="post" id="delete-event" action="event/delete?id=<%= x.id %>"/>
    <a href="/reservation/<%= x.id %>"><%= x.title %></a>
    <a class="btn btn-outline-warning" role="button" href="event/update?id=<%= x.id %>">ubah</a>
    <button class="btn btn-outline-danger" type="submit" form="delete-event">hapus</button>
  </li>
  <% }); %>
</ul>

<%- include('../components/footer') %>
